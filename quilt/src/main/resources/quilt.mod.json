{
  "schema_version": 1,
  "mixin": [
    "musicscript.mixins.json",
    "musicscript-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "${mod_id}",
    "version": "${version}",
    "metadata": {
      "name": "nova's music script",
      "description": "This is an example description! Tell everyone what your mod is about!",
      "contributors": {
        "null-version_nova": "Owner"
      },
      "contact": {
        "sources": "https://github.com/architectury/architectury-templates"
      },
      "icon": "assets/musicscript/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "org.nullversionnova.musicscript.quilt.MusicScriptQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      },
      {
        "id": "architectury",
        "version": ">=4.11.93"
      }
    ]
  },
  "minecraft": {
    "environment": "*"
  }
}