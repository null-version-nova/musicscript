{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "nova's music script",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "null-version_nova"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },
  "license": "MIT License",
  "icon": "assets/musicscript/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "org.nullversionnova.musicscript.fabric.MusicScriptFabric"
      }
    ]
  },
  "mixins": [
    "musicscript.mixins.json",
    "musicscript-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric-language-kotlin": ">=${fabric_kotlin_version}"
  }
}